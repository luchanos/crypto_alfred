#: keyboards/main_keyboard.py:9
msgid "My wallet 💰"
msgstr "ჩემი საფულე 💰"

#: keyboards/main_keyboard.py:10
msgid "Send 💸"
msgstr "გადარიცხვა 💸"

#: keyboards/main_keyboard.py:11
msgid "Referral link 🤝"
msgstr "რეფერალური ბმული 🤝"

#: keyboards/main_keyboard.py:12
msgid "Language 🌐"
msgstr "ენა 🌐"

#: keyboards/main_keyboard.py:13
msgid "Write to administration ✍️"
msgstr "წერილი ადმინისტრაციას ✍️"

#: keyboards/stop_send_rating_keyboard.py:7
msgid "Cancel ❌"
msgstr "გაუქმება ❌"

